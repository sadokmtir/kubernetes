{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::$MANAGEMENT_ACCOUNT_ID:root"
            },
            "Action": "sts:AssumeRole"
        }, 
         {
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::$ADMIN_ACCOUNT_ID:root"
            },
            "Action": "sts:AssumeRole"
        }
    ]
}